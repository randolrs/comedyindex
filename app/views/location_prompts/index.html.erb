<p id="notice"><%= notice %></p>

<h1>Listing Location Prompts</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Message</th>
      <th>Cta text</th>
      <th>Cta link</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_prompts.each do |location_prompt| %>
      <tr>
        <td><%= location_prompt.address %></td>
        <td><%= location_prompt.message %></td>
        <td><%= location_prompt.cta_text %></td>
        <td><%= location_prompt.cta_link %></td>
        <td><%= location_prompt.latitude %></td>
        <td><%= location_prompt.longitude %></td>
        <td><%= link_to 'Show', location_prompt %></td>
        <td><%= link_to 'Edit', edit_location_prompt_path(location_prompt) %></td>
        <td><%= link_to 'Destroy', location_prompt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location prompt', new_location_prompt_path %>
