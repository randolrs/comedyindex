<% if show.more_dates.count > 0 %>

  <h2>More Dates</h2>

  <% show.more_dates.each do |occurence| %>

    <p><%= occurence.start_time.strftime("%A, %b %e, %Y") %></p>

  <%end%>

  <% if user_signed_in? %><% if show.user_id == current_user.id %><p class="blue-text clickable profile-edit-action">Add Dates</p><%end%><%end%>

<%end%>
